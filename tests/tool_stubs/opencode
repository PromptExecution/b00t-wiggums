#!/usr/bin/env python3
"""Stub opencode CLI for integration testing."""

from __future__ import annotations

import sys

def main() -> int:
    sys.stdin.read()  # consume prompt input
    print("opencode-stub: starting", flush=True)
    print("<promise>COMPLETE</promise>", flush=True)
    return 0


if __name__ == "__main__":
    raise SystemExit(main())
